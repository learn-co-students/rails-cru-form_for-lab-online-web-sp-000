<h2>New Song Form</h2>

<%= form_for(@song) do |s| %>
    <label>Song Name:</label><br>
        <%= s.text_field :name %>

        <label>Artist ID:</label><br>
        <%= s.text_field :artist_id %>

        <label>Genre ID:</label><br>
        <%= s.text_field :genre_id %>

    <%= s.submit %>
<% end %>
<h1> Edit <%= @song.name %></h1>

<%= form_for(@song) do |s| %>
<%= s.label :name, 'Song Name:' %><br>
<%= s.text_field :name %><br>

<%= s.label :artist_id, 'Artist:' %>
<%= s.number_field :artist_id %><br>

<%= s.label :genre_id, 'Genre:' %>
<%= s.number_field :genre_id %><br>

<%= s.submit %>
<% end %>
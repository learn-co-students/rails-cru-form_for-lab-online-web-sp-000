<h1> Edit song form: </h1>
<%= form_for(@song) do |f| %>
    <%= f.label "Song name:" %><br>
    <%= f.text_field :name %><br>

    <%= f.label "Song artist_id:" %><br>
    <%= f.text_field :artist_id %><br>

    <%= f.label "Song genre_id:" %><br>
    <%= f.text_field :genre_id %><br>
    <%= f.submit %>
<% end %>
<h1>Edit Song Form</h1>

    <%= form_for @song do |f| %>
    <%= f.label "Song Name:" %> <br>
    <%= f.text_field :name %> <br>

    <%= f.label "Artist id:" %>
    <%= f.text_field :artist_id %>

    <%= f.label "Genre id:" %>
    <%= f.text_field :genre_id %>

    <%= f.submit "Update Song" %>

<% end %>
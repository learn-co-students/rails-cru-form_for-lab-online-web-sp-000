<h1>Songs#edit</h1>
<p>Find me in app/views/songs/edit.html.erb</p>
<h3>Edit Song Form</h3>

<%= form_for(@song) do |f| %>
  
  <label>song_name:</label><br>
  <%= f.text_field :name %><br>
  
  <label>song_artist_id:</label><br>
  <%= f.text_field :artist_id %><br>
  
<label>song_genre_id:</label><br>
  <%= f.text_field :genre_id %><br>

  <%= f.submit %>
<% end %>
<h1>Edit Song Form</h1>

<%= form_for(@song) do |s| %>
  <label>Name:</label><br>
  <%= s.text_field :name %><br>

  <label>Artist ID</label><br>
  <%= s.number_field :artist_id %><br>

  <label>Genre ID</label><br>
  <%= s.number_field :genre_id %><br>

  <%= s.submit "Update Song"%>
<% end %>
